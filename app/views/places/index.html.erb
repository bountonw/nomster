<div class="col-10 offset-1 text-right">
  <p><%= paginate @places %></p>
</div>

<% @places.each do |place| %>
  <div class="booyah-box col-10 offset-1">
    <h1><%= place.name %></h1>
    <i><%= place.address %></i><br />
    <p> <%= place.description %> </p><br />
  </div>
<% end %>


<div class="col-10 offset-1 pagination">
  <%= paginate @places %>
</div>